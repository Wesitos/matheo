{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "object-curly-spacing": 0,
    "func-names": ["error", "as-needed"]
  }
}
